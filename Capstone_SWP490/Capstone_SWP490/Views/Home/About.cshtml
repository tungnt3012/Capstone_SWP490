<script>
    GetListContent();
     function GetListContent() {
        $.ajax({
            url: '@Url.Action("GetContentAbout", "Home")',
            type: 'GET',
            success: function (results) {
                if (results != null) {
                    for (let i = 0; i < results.length; i++) {
                        console.log("" + results[i].content);
                        document.getElementById("page-content").innerHTML += `
                                        <div class="col-lg-12">
                                            <label class="Event-Schedule" for="Event_Schedule">
                                                `+ results[i].title+`
                                            </label>
                                        </div>
                                        <div class="col-lg-12">
                                            <div class="Event-Content" for="Event_Schedule">
                                                `+ results[i].content+`
                                            </div>
                                        </div>`;
                    }
                }
            }
        });
    };
</script>