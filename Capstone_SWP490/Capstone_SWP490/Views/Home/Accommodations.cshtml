<script>
    GetListContent();
    function GetListContent() {
        $.ajax({
            url: '@Url.Action("GetContentAccommodations", "Home")',
            type: 'GET',
            success: function (results) {
                if (results != null) {
                    for (let i = 0; i < results.length; i++) {
                        console.log("" + results[i].content);
                        document.getElementById("page-content-title").innerHTML += results[i].title;
                        document.getElementById("page-content-content").innerHTML += results[i].content;
                    }
                }
            }
        });
    }
</script>