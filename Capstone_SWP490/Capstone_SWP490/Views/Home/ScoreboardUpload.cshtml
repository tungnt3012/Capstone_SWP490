
@{
    ViewBag.Title = "Scoreboard | Upload";
}
<div class="container-fluid xyz">
    <div class="col-lg-12" id="content_area2_header">
        <div class="col-lg-12" id="content_area2_title">
            <label for="exampleInputTitle">Scoreboard Upload</label>
        </div>
    </div>
    <div class="main">
        <div class="row">
            <div class="col-lg-10 col-lg-offset-1" id="under">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                            <label for="firstName">Title scoreboard<span id="not-empty">*</span></label>
                            <input class="form-control" id="TitleScoreboard" name="TiLocationtle" , value="" placeholder="Scoreboard" required="required" />
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="form-group">
                            <label for="firstName">Link scoreboard<span id="not-empty">*</span></label>
                            <input class="form-control" id="TitleLinkScoreboard" name="TiLocationtle" , value="" placeholder="Link scoreboard" required="required" />

                        </div>
                    </div>
                    <div class="form-group">
                        <div class="pull-right">
                            <button type="button" class="btn btn-secondary" onclick="history.back()" ; return false;" id="event-canner">Cancel</button>
                            <button type="submit" class="btn btn-primary" id="event-upload">Save</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
