@{
    ViewBag.Title = "Registration";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Sidebar -->


<div id="page-content-wrapper">
    <div class="container-fluid xyz">
        <div class="col-lg-12" id="content_area2_header">
            <div class="col-lg-12" id="content_area2_title">
                <label for="exampleInputTitle">Registration</label>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="row">
                    <div class="col-lg-6">
                    </div>
                    <div class="col-lg-6" id="btn-registrantionTitle">
                        @using (Html.BeginForm("Result", "Registration", FormMethod.Post, new { enctype = "multipart/form-data" }))
                        {
                            <label for="formFile" class="form-label">Default file input <a href="#">example </a></label>
                            <br>
                            <input class="form-control" type="file" id="file" name="file">
                            <button class="btn btn-primary" value="Upload">Upload</button>
                        }
                    </div>
                </div>
                <div class="row">
                    Visit @Html.ActionLink("Registration Guide","Guide","Registration") for more detail
                    </div>
                </div>
            </div>
</div>


