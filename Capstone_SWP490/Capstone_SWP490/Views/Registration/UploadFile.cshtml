@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_mainLayout.cshtml";
}

<head>

</head>

<body>
    <nav class="navbar navbar-default no-margin">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header fixed-brand">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" id="menu-toggle">
                <span class="glyphicon glyphicon-th-large" aria-hidden="true"></span>
            </button>
        </div>
        <!-- navbar-header-->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <div class="content-area1-title">2021 ACM ICPC Asia-VietNam Regional Contest</div>
            <div class="content-area1-image"> <button type="button" class="btn-outline-login"><a href="https://localhost:44337/Home/HomeLogin" class="button">login</a></button></div>
            <div class="content-area1-image2"><img src="~/static/image/logofpt.png"></div>
            <ul class="nav navbar-nav">
                <li class="active">
                    <button class="navbar-toggle collapse in" data-toggle="collapse" id="menu-toggle-2">
                        <svg width="40" height="29" viewBox="0 0 40 29" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect width="40" height="28.9157" fill="url(#pattern0)" />
                            <defs>
                                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0_13_139" transform="translate(0 -0.191667) scale(0.00166667 0.00230556)" />
                                </pattern>
                                <image id="image0_13_139" width="600" height="600" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>
                    </button>
                </li>
            </ul>
        </div>
        <!-- bs-example-navbar-collapse-1 -->
    </nav>
    <div id="wrapper">
        <!-- Sidebar -->
        <div class="col-lg-12" id="content_area2_header">
            <div class="col-lg-12" id="content_area2_title">
                <label for="exampleInputTitle">Registration</label>
            </div>
        </div>
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav nav-pills nav-stacked" id="menu">
                <li class="no-bullet">
                    <img class="no-bullet-image" src="~/image/OLP_30_nam-removebg 1.png">
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Scoreboard</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Registration</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Schedule of activity</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Event</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Rules</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Local Information</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Accommodations</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Downloads</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Shirt-Sizing</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">About</button>
                </li>
            </ul>
        </div>
        <!-- /#sidebar-wrapper -->
        <!-- Page Content -->
        <div id="page-content-wrapper" style="padding-left: 20px">
            <div class="container-fluid xyz">
                <div class="row">
                    <div class="col-lg-12">
                        @if (Model == null)
                        {
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="form-group">
                                        <div class="mb-3">
                                            @using (Html.BeginForm("UploadFile", "Registration", FormMethod.Post, new { enctype = "multipart/form-data" }))
                                            {
                                                <label for="formFile" class="form-label">Default file input <a href="#">example </a></label>
                                                <input class="form-control" type="file" id="file" name="file">
                                                <input type="submit" value="Upload" />
                                            }
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="form-group">

                                    </div>
                                </div>
                            </div>
                        }
                    <div class="row">
                        @if (Model != null)
                        {
                            <div>- School: @Model.school.school_name</div>
                            <div>- Address: @Model.school.address</div>
                            <div>- Coach: @(Model.coach.first_name +" "+ Model.coach.middle_name +" "+ Model.coach.last_name) - Email: @Model.coach.email - Phone: @Model.coach.phone_number</div>
                            <div>- Vice Coach: @(Model.vice_coach.first_name +" "+ Model.vice_coach.middle_name +" "+ Model.vice_coach.last_name) - Email: @Model.vice_coach.email - Phone: @Model.vice_coach.phone_number</div>
                        }
                        <div class="col-lg-12">
                            <table class="table table-striped">
                                <thead>
                                    <tr>
                                        <th scope="col">Team</th>
                                        <th scope="col">member role</th>
                                        <th scope="col">First name</th>
                                        <th scope="col">middle name</th>
                                        <th scope="col">Lirst name</th>
                                        <th scope="col">Date of birth</th>
                                        <th scope="col">email</th>
                                        <th scope="col">phone number</th>
                                        <th scope="col">ICPC ID</th>
                                        <th scope="col">Gender</th>
                                        <th scope="col">Year</th>
                                        <th scope="col">Award</th>
                                        <th scope="col"></th>
                                    </tr>
                                </thead>
                                @if (Model != null)
                                {
                            <tbody>

                                @foreach (var team in Model.team_member.Keys)
                                {
                                    foreach (var item in Model.team_member[team])
                                    { <tr>
                                            <th scope="row">@team.team_name</th>
                                            <td>@(item.member_role == 1 ? "Coach" : item.member_role == 2 ? "Vice Coach" : item.member_role == 3 ? "Leader" : "Member" )</td>
                                            <td>@(item.first_name == "" ? "N/A" : item.first_name)</td>
                                            <td>@(item.middle_name == "" ? "N/A" : item.middle_name)</td>
                                            <td>@(item.last_name == "" ? "N/A" : item.last_name)</td>
                                            <td>@(item.dob == null ? "N/A" : item.dob)</td>
                                            <td>@(item.email == "" ? "N/A" : item.email)</td>
                                            <td>@item.phone_number</td>
                                            <td>@(item.phone_number == "" ? "N/A" : item.phone_number) </td>
                                            <td>@(item.gender == 0 ? "Male" : "Female")</td>
                                            <td>@(item.year == 0 ? "N/A" : item.year)</td>
                                            <td>@(item.award == "" ? "" : "")</td>
                                            <td><a href="https://localhost:44337/Home/RegistrationFrom">Detail</a></td>
                                        </tr>
                                    }
                                }
                            </tbody>
                                }
                            </table>
                        </div>
                    </div>

                        </div>
                </div>
            </div>
        </div>
        <!-- /#page-content-wrapper -->
    </div>
    <!-- /#wrapper -->
    <!-- jQuery -->

</body>

