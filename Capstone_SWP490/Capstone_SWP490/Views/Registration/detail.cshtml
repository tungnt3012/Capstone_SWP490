                    <button type="button" class="btn btn-outline-succes">Registration</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Schedule of activity</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Event</button>
                </li>
                <li class="no-bullet">
                    <button type="button" class="btn btn-outline-succes">Rules</button>
                </li>
