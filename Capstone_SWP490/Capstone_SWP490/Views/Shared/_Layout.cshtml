<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jquery")
    @*<link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>*@
    @*<link href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@1,700&display=swap" rel="stylesheet">*@
    <link rel="stylesheet" href="~/Content/home.css" asp-append-version="true" />
</head>
<body>
    <nav class="navbar navbar-default no-margin">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header fixed-brand">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" id="menu-toggle">
                <span class="glyphicon glyphicon-th-large" aria-hidden="true"></span>
            </button>
            <div class="content-area1-title">2021 ACM ICPC Asia-VietNam Regional Contest</div>
        </div>
        <!-- navbar-header-->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <div class="content-area1-title">2021 ACM ICPC Asia-VietNam Regional Contest</div>
            <div class="content-area1-image">

                @*<button class="btn btn-outline-login" onclick="location.href='@Url.Action("Login", "Login")';return false;">Login</button>*@
                <div class="btn-group">
                    <button type="button" id="login-user" class="btn btn-secondary dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <i class="arrow down"></i>
                    </button>
                    <div class="dropdown-menu dropdown-menu-right">
                        <button class="dropdown-item" type="button"><div class="col-lg-12" id="user-information">Dao Quang Son <div class="col-lg-12" id="user-role">role: Admin</div></div>
                       </button>
                        <button class="dropdown-item" type="button" id="user-changpassword"><div class="col-lg-12">Change password</div></button>
                    <button class="dropdown-item" type="button" id="user-logout"><div class="col-lg-12">log out</div></button>
                    </div>
                </div>
            </div>
            <div class="content-area1-image2"><img src="~/image/logofpt.png"></div>

            <ul class="nav navbar-nav">
                <li class="active">
                    <button class="navbar-toggle collapse in" data-toggle="collapse" id="menu-toggle-2">
                        <svg width="40" height="29" viewBox="0 0 40 29" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect width="40" height="28.9157" fill="url(#pattern0)" />
                            <defs>
                                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0_13_139" transform="translate(0 -0.191667) scale(0.00166667 0.00230556)" />
                                </pattern>
                                <image id="image0_13_139" width="600" height="600" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>

                    </button>
                </li>
            </ul>
        </div>
        <!-- bs-example-navbar-collapse-1 -->
    </nav>
    <div id="wrapper">
        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav nav-pills nav-stacked" id="menu">

                <li id="menu-scoreboard" onclick="location.href='@Url.Action("#", "Home")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>Scoreboard</a>
                </li>
                <li id="menu-registration" onclick="location.href='@Url.Action("", "Registration")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>Registration</a>
                </li>
                <li id="menu-scheduleofactivities" onclick="location.href='@Url.Action("ScheduleOfActivities", "Home")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>Schedule Of Activities</a>
                </li>
                <li onclick="location.href='@Url.Action("Event", "Home")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>Event</a>
                </li>
                <li onclick="location.href='@Url.Action("Rules", "Home")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>Rules</a>
                </li>
                <li onclick="location.href='@Url.Action("LocalInformation", "Home")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>Local Information</a>
                </li>
                <li onclick="location.href='@Url.Action("Accommodations", "Home")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>Accommodations</a>
                </li>
                <li onclick="location.href='@Url.Action("Downloads", "Home")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>Downloads</a>
                </li>
                <li onclick="location.href='@Url.Action("ShirtSizing", "Home")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>Shirt Sizing</a>
                </li>
                <li onclick="location.href='@Url.Action("About", "Home")';return false;">
                    <a href="#"><span class="fa-stack fa-lg pull-left"><i class="fa fa-cloud-download fa-stack-1x "></i></span>About</a>
                </li>
            </ul>
        </div>
        @RenderBody()
    </div>
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/home.js" asp-append-version="true"></script>

    <script src="~/Scripts/bootstrap.js" asp-append-version="true"></script>


</body>
</html>
