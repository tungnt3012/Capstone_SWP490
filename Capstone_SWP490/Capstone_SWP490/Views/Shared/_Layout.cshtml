<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jquery")
    @*<link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>*@
    @*<link href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@1,700&display=swap" rel="stylesheet">*@
    <link rel="stylesheet" href="~/Content/home.css" asp-append-version="true" />
</head>
<body>
    <nav class="navbar navbar-default no-margin">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header fixed-brand">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" id="menu-toggle">
                <span class="glyphicon glyphicon-th-large" aria-hidden="true"></span>
            </button>
        </div>
        <!-- navbar-header-->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <div class="content-area1-title">2021 ACM ICPC Asia-VietNam Regional Contest</div>
            <div class="content-area1-image">
                @*<button type="button" class="btn-outline-login"><a href="https://localhost:44337/Home/HomeLogin" class="button">login</a></button>*@
                <button class="btn btn-outline-login" onclick="location.href='@Url.Action("Login", "Login")';return false;">Login</button>
            </div>
            <div class="content-area1-image2"><img src="~/image/logofpt.png"></div>

            <ul class="nav navbar-nav">
                <li class="active">
                    <button class="navbar-toggle collapse in" data-toggle="collapse" id="menu-toggle-2">
                        <svg width="40" height="29" viewBox="0 0 40 29" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect width="40" height="28.9157" fill="url(#pattern0)" />
                            <defs>
                                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0_13_139" transform="translate(0 -0.191667) scale(0.00166667 0.00230556)" />
                                </pattern>
                                <image id="image0_13_139" width="600" height="600" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>

                    </button>
                </li>
            </ul>
        </div>
        <!-- bs-example-navbar-collapse-1 -->
    </nav>
    <div id="wrapper">
        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav nav-pills nav-stacked" id="menu">
                <li class="no-bullet">
                    <img class="no-bullet-image" src="~/image/OLP_30_nam-removebg 1.png">
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-scoreboard" onclick="location.href='@Url.Action("#", "Home")';return false;">Scoreboard</button>
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-registration" onclick="location.href='@Url.Action("", "Registration")';return false;">Registration</button>
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-scheduleofactivities" onclick="location.href='@Url.Action("ScheduleOfActivities", "Home")';return false;">Schedule Of Activities</button>
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-event" onclick="location.href='@Url.Action("Event", "Home")';return false;">Event</button>
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-rules" onclick="location.href='@Url.Action("Rules", "Home")';return false;">Rules</button>
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-localinformation" onclick="location.href='@Url.Action("LocalInformation", "Home")';return false;">Local Information</button>
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-accommodations" onclick="location.href='@Url.Action("Accommodations", "Home")';return false;">Accommodations</button>
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-downloads" onclick="location.href='@Url.Action("Downloads", "Home")';return false;">Downloads</button>
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-shirtsizing" onclick="location.href='@Url.Action("ShirtSizing", "Home")';return false;">Shirt Sizing</button>
                </li>
                <li class="no-bullet">
                    <button class="btn btn-outline-succes" id="menu-about" onclick="location.href='@Url.Action("About", "Home")';return false;">About</button>
                </li>
            </ul>
        </div>
        @RenderBody()
    </div>
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/home.js" asp-append-version="true"></script>
</body>
</html>
