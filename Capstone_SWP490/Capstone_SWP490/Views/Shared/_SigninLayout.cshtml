@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jquery")
    <link rel="stylesheet" href="~/Content/home.css" asp-append-version="true" />
</head>
<body>
    <nav class="navbar navbar-default no-margin">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header fixed-brand">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" id="menu-toggle">
                <span class="glyphicon glyphicon-th-large" aria-hidden="true"></span>
            </button>
        </div>
        <!-- navbar-header-->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <div class="content-area1-title">2021 ACM ICPC Asia-VietNam Regional Contest</div>
            <div class="content-area1-image"> <button type="button" class="btn-outline-login">login</button></div>
            <div class="content-area1-image2"><img src="~/image/logofpt.png"></div>

            <ul class="nav navbar-nav">
                <li class="active">
                    <button class="navbar-toggle collapse in" data-toggle="collapse" id="menu-toggle-2">
                        <svg width="40" height="29" viewBox="0 0 40 29" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect width="40" height="28.9157" fill="url(#pattern0)" />
                            <defs>
                                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0_13_139" transform="translate(0 -0.191667) scale(0.00166667 0.00230556)" />
                                </pattern>
                                <image id="image0_13_139" width="600" height="600" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>
                    </button>
                </li>
            </ul>
        </div>
    </nav>
    @RenderBody()
    <script src="~/Scripts/home.js" asp-append-version="true"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
